{
    "name": "sy-records/webhooks",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "webhook",
        "github",
        "gitee",
        "gitlab"
    ],
    "description": "WebHook Handler for GitHub, Gitee and GitLab.",
    "authors": [
        {
            "name": "Lu Fei",
            "email": "i@lufei.dev"
        }
    ],
    "autoload": {
        "psr-4": {
            "Luffy\\WebHook\\": "src/"
        }
    },
    "require": {
        "php": ">=7.2",
        "guzzlehttp/psr7": "^2.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "cs-fix": "php-cs-fixer fix $1"
    }
}
